{
	"name": "terranova.client",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "run-script-os",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"prestart": "node aspnetcore-https",
		"start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
		"start:linux": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
		"docs": "compodoc -p tsconfig.json -s",
		"serve:ssr:terranova.client": "node dist/terranova.client/server/server.mjs"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^19.2.2",
