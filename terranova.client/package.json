    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.2",
    "@angular/localize": "^19.2.2",
    "@compodoc/compodoc": "^1.1.26",
    "@spartan-ng/cli": "^0.0.1-alpha.424",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.7",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
