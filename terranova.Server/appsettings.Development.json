{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
    "ConnectionStrings": {
        "DevDB": "Server=${SQL_SERVER};Database=${SQL_NAME};User ID=${SQL_USER};Password=${SQL_PASSWORD};TrustServerCertificate=True;MultipleActiveResultSets=true;",
        "BlobAzuriteConnectionString": "${BLOB_CONNECTION}"
    },
  "AppSettings": {
    "JWT_Secret": "${JWT_SECRET}"
  },
  "AdminCredentials": {
    "Username": "${ADMIN_USERNAME}",
    "Email": "${ADMIN_EMAIL}",
    "Password": "${ADMIN_PASSWORD}"
  }
}